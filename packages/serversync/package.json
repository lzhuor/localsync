{
  "name": "serversync",
  "version": "2.0.2",
  "description": "a lightweight server-side module to sync JS objects in realtime across tabs / windows of a browser.",
  "main": "lib/index.js",
  "files": [
    "lib",
    "src",
    "doc"
  ],
  "keywords": [
    "sync",
    "local",
    "storage",
    "cookies",
    "stream",
    "ie",
    "fallback"
  ],
  "scripts": {
    "start": "yarn build --watch",
    "clean": "rm -rf lib tsconfig.tsbuildinfo",
    "build": "tsc --build",
    "prerelease": "yarn build",
    "release": "yarn publish --patch --non-interactive --no-git-tag-version --access public"
  },
  "dependencies": {
    "localsync-core": "^2.0.0",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@types/node": "^12.6.9",
    "typescript": "^3.5.3"
  },
  "author": "Cole Chamberlain <colewc@google.com> (https://github.com/cchamberlain)",
  "bugs": {
    "url": "https://github.com/noderaider/localsync/issues"
  },
  "homepage": "http://localsync.js.org",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/noderaider/localsync.git"
  }
}
